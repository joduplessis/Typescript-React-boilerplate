{
  "name": "Typescript & React boilerplate",
  "version": "1.0.0",
  "description": "Typescript, React, Redux, Jest, Webpack & GitLAB CI boilerplate project.",
  "main": "index.js",
  "repository": "https://github.com/joduplessis/Typescript-React-boilerplate",
  "author": "Jo du Plessis",
  "license": "MIT",
  "dependencies": {
    "@types/crypto-js": "^3.1.33",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/promise": "^7.1.30",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-redux": "^4.4.40",
    "@types/react-router": "3.0.2",
    "@types/react-transition-group": "^1.1.0",
    "awesome-typescript-loader": "^3.1.2",
    "axios": "^0.16.1",
    "crypto-js": "^3.1.9-1",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^3.1.0",
    "html-loader": "^0.4.5",
    "jasmine": "^2.6.0",
    "jest": "^20.0.4",
    "jsdom": "^10.1.0",
    "live-server": "^1.2.0",
    "merge-stream": "^1.0.1",
    "moment": "^2.18.1",
    "momentjs": "^2.0.0",
    "promise": "^7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.4",
    "react-router": "3.0.2",
    "react-transition-group": "^1.1.3",
    "recharts": "^0.22.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sha512": "^0.0.1",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "scripts": {
    "test": "jest -u",
    "gulp": "node ./node_modules/gulp/bin/gulp.js",
    "serve": "node ./node_modules/live-server/live-server.js --mount=/:./dist --host=localhost --port:8000",
    "watch-dev-both": "npm run gulp | npm run watch-dev",
    "watch-dev": "webpack -w --config webpack.config.js",
    "watch-production": "webpack -w --config webpack.config.production.js",
    "build-dev": "webpack --config webpack.config.js",
    "build-production": "webpack --config webpack.config.production.js"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
